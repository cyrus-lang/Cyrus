extern fn printf(format: const char*, ...);
extern fn exit(status: int);

struct Person {
    name: string;

    public fn display(age: int) {
        printf("name: %s\n", (*this).name);
        
        // (*this).name = "Change!";
        this->name = "Change!";

        printf("age: %d\n", age);
    }
}

fn main() {
    #user = Person { name: "Taha" };
    #ref = &user;

    ref.display(10);
    // user.display(18);
}