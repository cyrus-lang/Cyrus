public type FILE = void*;

public extern stdout: FILE;
public extern stderr: FILE;
public extern stdin: FILE;

public extern func printf(format: const char*, ...): int32;
public extern func fprintf(stream: FILE, format: const char*, ...): int32;

public extern func malloc(size: int32): void*;
public extern func free(ptr: void*): void;

public extern func exit(status: int32): void;